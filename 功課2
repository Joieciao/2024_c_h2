#include <iostream>

int main()
{
    int x,y,r;
    
    printf("please input a number:");
    scanf("%d",&x);
    printf("please input another number:");
    scanf("%d",&y);
    printf("please input the sum:");
    scanf("%d",&r);
    
    if(x+y==r){
        printf("right");
    }else{
        printf("error");
    }
    return 0;
}
